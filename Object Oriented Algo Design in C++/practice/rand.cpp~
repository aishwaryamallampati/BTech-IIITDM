#include<iostream>
#include<cstdlib>
#include<time.h>

using namespace std;

main()
{
	int index1,size,i;
	size=20;
	i=1;
	time_t t;
	time(&t);
	srand((unsigned int)t);
	while(size<=(100*size))
	{
	 cout<<"\nthe elements of size %d: "<<(i*size);
	 for(index1=0;index1<(i*size);index1++)
	 {
		cout<<rand()<<" ";
	 }
	 cout<<endl;
	 i++;
	}	
	return 0;
}


